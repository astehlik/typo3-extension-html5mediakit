{
	"name": "de-swebhosting-typo3-extension/html5mediakit",
	"description": "Allows easy embedding of HTML5 video and audio elements.",
	"type": "typo3-cms-extension",
	"license": "GPL-3.0+",
	"require": {
		"php": ">=7.0.0",
		"typo3/cms-core": "^7.6|^8.7|^9.3|dev-master"
	},
	"require-dev": {
		"namelesscoder/typo3-repository-client": "^1.1.0",
		"phpunit/phpunit": "^5.6",
		"squizlabs/php_codesniffer": "^2.6",
		"de-swebhosting/php-codestyle": "dev-master"
	},
	"replace": {
		"html5mediakit": "self.version",
		"typo3-ter/html5mediakit": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Sto\\Html5mediakit\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Sto\\Html5mediakit\\Tests\\": "Tests"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
